{
	"name": "metronic-tailwind-vue",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc -b && vite build",
		"preview": "vite preview",
		"build:css": "npx @tailwindcss/cli -i ./src/metronic/css/styles.css -o ./src/metronic/dist/styles.css",
		"build:css:watch": "npx @tailwindcss/cli -i ./src/metronic/css/styles.css -o ./src/metronic/dist/styles.css --watch",
		"build:prod": "npx @tailwindcss/cli -i ./src/metronic/css/styles.css -o ./src/metronic/dist/styles.css --minify && npx webpack --env production",
		"format": "prettier --check --ignore-path .gitignore .",
		"format:fix": "prettier --write --ignore-path .gitignore .",
		"lint": "npx prettier"
	},
	"dependencies": {
		"@babel/core": "^7.27.1",
		"@babel/preset-env": "^7.27.1",
		"@eslint/js": "^9.25.1",
		"@keenthemes/ktui": "^1.0.11",
		"@popperjs/core": "^2.11.8",
		"@tailwindcss/cli": "^4.1.5",
		"@tailwindcss/forms": "^0.5.10",
		"babel-loader": "^10.0.0",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^13.0.0",
		"css-loader": "^7.1.2",
		"eslint": "^9.25.1",
		"mini-css-extract-plugin": "^2.9.2",
		"mini-svg-data-uri": "^1.4.4",
		"postcss-import": "^16.1.0",
		"postcss-loader": "^8.1.1",
		"postcss-nesting": "^13.0.1",
		"postcss-preset-env": "^10.1.6",
		"source-map-loader": "^5.0.0",
		"style-loader": "^4.0.0",
		"terser-webpack-plugin": "^5.3.14",
		"ts-loader": "^9.5.2",
		"typescript": "^5.8.3",
		"vue": "^3.5.13",
		"webpack": "^5.99.7",
		"webpack-cli": "^6.0.1",
		"webpack-merge-and-include-globally": "^2.3.4"
	},
	"devDependencies": {
		"@keenthemes/ktui": "^1.0.8",
		"@popperjs/core": "^2.11.8",
		"@vitejs/plugin-vue": "^5.2.3",
		"@vue/tsconfig": "^0.7.0",
		"apexcharts": "^4.7.0",
		"autoprefixer": "^10.4.21",
		"axios": "^1.9.0",
		"clipboard": "^2.0.11",
		"esri-leaflet": "^3.0.16",
		"esri-leaflet-geocoder": "^3.1.5",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"tailwindcss": "^4.1.7",
		"typescript": "~5.8.3",
		"vite": "^6.3.5",
		"vue-tsc": "^2.2.8"
	}
}
